.gray-bg{background: #f3f3f3}
.order-title{background: #fff;padding: 35px 0;text-align: center;margin-bottom: 12px;}
.order-title .red-color{font-size: 35px;font-weight: bold}
.order-title .title-tip{font-size: 14px;}
.myorder{background: #fff;padding:20px 16px;font-size: 14px;}
.myorder .gray-color{display: block;float: right;font-size: 14px;}
.orderwait{padding: 15px 16px;font-size: 14px;}
.orderlist{background: #fff;text-align: center;font-size: 12px;}
.orderlist .row{padding:0;height: 50px;line-height: 50px;}
.orderlist .row .col{border-left: 1px solid #f2f2f2; padding: 0;}
.orderlist .row .col:first-child{border-left: 0}
.orderlist .row.secondrow .col{border-top: 1px solid #f2f2f2;}
.orderNum{background: #fff;text-align: center}
.orderNum .row{padding: 0}
.orderNum .row .col{padding: 20px 0;border-left:1px solid #f2f2f2;font-size: 12px;}
.orderNum .row .col:first-child{border-left: 0}
.orderNum .row .col image{height: 25px;width: 25px;margin-bottom: 5px;}
.orderNum .row.secondrow .col{border-top: 1px solid #f2f2f2;}
